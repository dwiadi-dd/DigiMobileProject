    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@components": "./src/components",
          "@constant": "./src/constant",
          "@screens": "./src/screens",
          "@utils": "./src/utils",
          "@contexts": "./src/contexts",
          "@hooks": "./src/hooks",
          "@stores": "./src/stores",
          "@navigations": "./src/navigations",
          "@services": "./src/services"
        }
      }
    ]
  ]
}
